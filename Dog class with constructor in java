class Dog {
    private String name;
    private String color;

    public Dog(String name, String color) {
        this.name = name;
        this.color = color;
    }

    public void display() {
        System.out.println("Dog Name: " + name);
        System.out.println("Dog Color: " + color);
    }
    public static void main(String[] args) {
        Dog myDog = new Dog("Buddy", "Brown");
        myDog.display();
    }
}
